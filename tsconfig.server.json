{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist-server/",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "module": "commonjs",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "radweb": [
        "../radweb/projects/radweb/src/public_api"
      ],
      "radweb-server": [
        "../radweb/projects/radweb-server"
      ],
      "radweb-server-postgres": [
        "../radweb/projects/radweb-server-postgres"
      ],
      "radweb/*": [
        "../radweb/projects/radweb/src/*"
      ],
    }
  },
  "files": [
    "./src/app/server/server.ts",
    "./src/app/server/doSomething.ts"
  ]
}